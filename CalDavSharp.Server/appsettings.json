{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "File": {
            "Path": "app.log",
            "Append": "True",
            "FileSizeLimitBytes": 0, // use to activate rolling file behaviour
            "MaxRollingFiles": 0 // use to specify max number of log files
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Data Source={AppDir}CalDavServer.sqlite;Version=3;"

    }
}